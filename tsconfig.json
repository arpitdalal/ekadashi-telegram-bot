{
  "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "rootDir": "src",
      "outDir": "dist",
      "strict": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true,
    },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}